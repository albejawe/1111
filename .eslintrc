{
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react-hooks"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": "off",
    "react-hooks/rules-of-hooks": "error", // Checks rules of Hooks
    "react-hooks/exhaustive-deps": "warn", // Checks effect dependencies
    "quotes": [1, "double"]
  }
}